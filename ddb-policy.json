{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "DynamoDBAccess",
            "Effect": "Allow",
            "Action": [
                "dynamodb:PutItem",
                "dynamodb:GetItem",
                "dynamodb:DescribeTable",
                "dynamodb:Scan",
                "dynamodb:Query"
            ],
            "Resource": ["arn:aws:dynamodb:eu-west-1:151580225166:table/service_registry",
                         "arn:aws:dynamodb:eu-west-1:151580225166:table/service_status"]

        }
    ]
}
